// app/(app)/financeiro/page.tsx
import { useAppStore } from './store'; // Assuming useAppStore is imported from a store file

const FinanceiroPage = () => {
function setEntries(rows: any[]) {
  useAppStore.setState((prev: any) => ({
    finance: { ...(prev.finance ?? {}), entries: rows },
  }));
}

// /** rest of code here **/

return (
  <div>
    {/* Component code here */}
  </div>
);
};

export default FinanceiroPage;

